@article{hafner-burton2009,
  title = {Network {{Analysis}} for {{International Relations}}},
  author = {Hafner-Burton, Emilie M. and Kahler, Miles and Montgomery, Alexander H.},
  date = {2009-07},
  journaltitle = {International Organization},
  shortjournal = {Int Org},
  volume = {63},
